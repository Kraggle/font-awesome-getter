{
	"name": "simple-mix-watcher",
	"version": "1.0.0",
	"description": "This is used to create an environment to just watch the directory for auto reload.",
	"author": "Kraggle",
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "npm run production",
		"production": "mix --production",
		"build": "build-esm"
	},
	"devDependencies": {
		"@types/jquery": "^3.5.6",
		"autoprefixer": "^10.3.1",
		"browser-sync": "^2.27.5",
		"browser-sync-webpack-plugin": "^2.3.0",
		"laravel-mix": "^6.0.6",
		"postcss": "^8.1.14",
		"resolve-url-loader": "^4.0.0",
		"sass": "^1.36.0"
	},
	"browserslist": [
		"defaults",
		"not ie < 11",
		"last 2 versions",
		"> 1%",
		"iOS 7",
		"last 3 iOS versions"
	]
}